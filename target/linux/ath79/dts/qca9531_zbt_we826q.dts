/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca953x.dtsi"

/ {
	compatible = "zbt,we826q", "qca,qca9531";
	model = "ZBT WE826-Q";

	aliases {
		label-mac-device = &wmac;
	};
	leds {
		compatible = "gpio-leds";

		status {
			label = "zbt-we826q:green:status";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		wlan2g {
			label = "zbt-we826q:green:wlan-2g";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};

		lan1 {
			label = "zbt-we826q:green:lan1";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		lan2 {
			label = "zbt-we826q:green:lan2";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		lan3 {
			label = "zbt-we826q:green:lan3";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		lan4 {
			label = "zbt-we826q:green:lan4";
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
		};

		wan {
			label = "zbt-we826q:green:wan";
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "WPS button";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		}

	};

	watchdog {
		compatible = "linux,wdt-gpio";

		gpios = <&gpio 2 GPIO_ACTIVE_HIGH>;
		hw_algo = "toggle";
		hw_margin_ms = <1200>;
		always-running;
	};
};

&uart {
	status = "okay";
};

&usb0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&pcie0 {
	status = "okay";
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x1000>;
};

&mdi0 {
	status = "okay";
};